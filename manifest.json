{
  "manifest_version": 2,
  "name": "mangaTracker",
  "version": "0.1",
  "content_scripts": [
		{
			"matches": [
			  "<all_urls>"
			],
			"js": ["jquery-3.4.1.min.js", "content.js", "popup.js"]
		}
  ],
  "browser_action": {
	  "default_popup": "popup.html",
  	"default_icon": "icon.png"
	},
	"background": {
  	"scripts": ["background.js"]
	},
	"permissions": [
		"tabs",
		"storage"
	]
}